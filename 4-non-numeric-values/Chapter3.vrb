\frametitle{Logical comparisons}

\begin{itemize}

\item Alphabetical order matters:

\begin{verbatim}
> "alpha"<="beta"
[1] TRUE
> "gamma">"Alpha"
[1] TRUE
\end{verbatim}

\item Uppercase letters also matters:
\begin{verbatim}
> "Alpha">"alpha"
[1] TRUE
> "beta">="bEtA"
[1] FALSE
\end{verbatim}

\end{itemize}


